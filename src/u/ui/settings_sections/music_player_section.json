  "music_player_button_panel_base": {
    "type": "panel",
    "size": [ "100%", 30 ],
    "$is_custom_song|default": false,
    "controls": [
      {
        "album_image_panel": {
          "type": "panel",
          "offset": [ "-50% + 16px", 0 ],
          "layer": 50,
          "controls": "$album_image_panel_controls"
        }
      },
      {
        "type_image_panel": {
          "type": "panel",
          "offset": [ "50% - 16px", 0 ],
          "layer": 50,
          "controls": "$type_image_panel_controls",
          "$type_image_panel_controls|default": [
            {
              "speaker@music_player_section.speaker_image": {}
            }
          ]
        }
      },
      {
        "player@settings_common.action_button": {
          "$border_visible": false,
          "$button_text": " ",
          "$default_button_texture|default": "textures/ui/empty",
          "$hover_button_texture|default": "textures/ui/focus_border_white",
          "$pressed_button_texture|default": "textures/ui/focus_border_green",
          "$locked_button_texture|default": "textures/ui/empty",
          "$focus_override_right": "FOCUS_OVERRIDE_STOP",
          "sound_name": "$song_sound_event",
          "sound_pitch": 1,
          "sound_volume": 1,
          "layer": 6
        }
      },
      {
        "song_name": {
          "type": "label",
          "text": "$song_name",
          "size": [ "default", 10 ],
          "max_size": [ "100%", "100%" ],
          "offset": "$song_name_offset",
          "layer": 50,
          "anchor_from": "center",
          "anchor_to": "center",
          "$song_name_offset|default": [ 0, -5 ],
          "variables": [
            {
              "requires": "$is_custom_song",
              "$song_name_offset": [ 0, 0 ]
            }
          ]
        }
      },
      {
        "artist_name": {
          "type": "label",
          "text": "$artist_name",
          "size": [ "default", 10 ],
          "max_size": [ "100%", "100%" ],
          "offset": [ 0, 5 ],
          "layer": 50,
          "anchor_from": "center",
          "anchor_to": "center",
          "ignored": "$is_custom_song"
        }
      }
    ]
  },

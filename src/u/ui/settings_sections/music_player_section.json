            },
            { "small_padding@how_to_play_common.small_padding": {
                "ignored": "(not $always_show_custom_section)"
              }
            },
            { "padding@how_to_play_common.padding": {
                "ignored": "$always_show_custom_section"
              }
            }
          ],
          "bindings": "$information_section_bindings",
          "$information_section_bindings|default": [
            {
              "binding_type": "view",
              "source_control_name": "music_player_section.information_section_toggle",
              "source_property_name": "#toggle_state",
              "target_property_name": "#visible"
            }
          ],
          "variables": [
            {
              "requires": "$always_show_information_section",
              "$information_section_bindings": []
            }
          ]
        }
      },
      {
        "custom_section_toggle@settings_common.option_toggle": {
